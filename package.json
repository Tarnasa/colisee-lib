  },
  "homepage": "https://github.com/siggame/colisee-lib#readme",
  "devDependencies": {
    "@types/chai": "^4.0.4",
    "@types/express": "^4.0.37",
    "@types/knex": "0.0.60",
    "@types/lodash": "^4.14.74",
    "@types/mocha": "^2.2.42",
    "@types/node": "^8.0.25",
    "@types/winston": "^2.3.5",
    "chai": "^4.1.1",
    "mocha": "^3.5.0",
    "typedoc": "^0.8.0",
    "typescript": "^2.4.2"
